*filter
:INPUT DROP [0:0]
:FORWARD DROP [0:0]
:OUTPUT ACCEPT [0:0]
{% if docker_installed.stat.exists %}
:DOCKER-USER - [0:0]
{% endif %}
-A INPUT ! -i {{interface_name}} -j ACCEPT
-A INPUT -m state --state RELATED,ESTABLISHED -j ACCEPT
-A INPUT -p tcp -m tcp --dport 22 -j ACCEPT
-A INPUT -p icmp -j ACCEPT
{% for rule in iptables_input_rules %}
-I INPUT {{ rule }}
{% endfor %}
{% if docker_installed.stat.exists %}
-A DOCKER-USER -o {{interface_name}} -j RETURN
-A DOCKER-USER -i {{interface_name}} -m conntrack --ctstate RELATED,ESTABLISHED -j ACCEPT
-A DOCKER-USER -i {{interface_name}} -j DROP
{% for rule in iptables_docker_user_rules %}
-I DOCKER-USER {{ rule }}
{% endfor %}
{% endif %}
